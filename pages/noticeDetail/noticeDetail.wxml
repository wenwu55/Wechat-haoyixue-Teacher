<!--pages/noticeDetail/noticeDetail.wxml-->
<view class="article">
  <view class="title">{{title}}</view>
  <view class="date">{{createtime}}</view>

  <view class="detail">{{content}}</view>
  <image class="pics" wx:for="{{imageList}}" wx:key="{{index}}" src="{{item}}" bindtap='previewImage' data-src="{{item}}"></image>
</view>