/**app.wxss**/
@import "WeUI.wxss";
page {
  background-color: #F4F5F9;
}
.default {
  color: red;
}
.textRight {
  text-align: right;
}

.default {
  padding-top: 203rpx;
  text-align: center;
}

.nothing {
  width: 360rpx;
  height: 207rpx;
  display: inline-block;
  margin-bottom: 16rpx;
}

.text {
  color: #999999;
  font-size: 28rpx;
  line-height: 50rpx;
}

.addBtn {
background-color: #4B87FF;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  color: #fff;
  font-size: 30rpx;
  line-height: 88rpx;
  font-weight: 600;
}

/**app.wxss**/
.container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
